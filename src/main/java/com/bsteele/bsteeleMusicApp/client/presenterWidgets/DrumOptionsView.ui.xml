<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="resources" type="com.bsteele.bsteeleMusicApp.client.resources.AppResources"/>

    <g:HTMLPanel>
        <p>
            HighHat:
            <select ui:field="highHatSelect" >
                <option value="____" selected="true">Off</option>
                <option value="x___" >1</option>
                <option value="x___x___" >1 3</option>
                <option value="__x___x_" >2 4</option>
                <option value="x_x_x_x_" >All 4's</option>
                <option value="xxxxxxxx" >All 8's</option>
                <option value="XsXsXsXs" >Swing 8's</option>
            </select>
        </p>
        <p>
            Snare:
            <select ui:field="snareSelect" >
                <option value="____" selected="true">Off</option>
                <option value="x___" >1</option>
                <option value="__x_" >3</option>
                <option value="x_x_" >1 3</option>
                <option value="_x_x" >2 4</option>
                <option value="xxxx" >All 4's</option>
            </select>
        </p>
        <p>
            Kick:
            <select ui:field="kickSelect" >
                <option value="____" selected="true">Off</option>
                <option value="x___" >1</option>
                <option value="__x_" >3</option>
                <option value="x_x_" >1 3</option>
                <option value="x___xx__" >1 double3</option>
                <option value="_x_x" >2 4</option>
                <option value="xxxx" >All 4's</option>
            </select>
        </p>
    </g:HTMLPanel>
</ui:UiBinder>